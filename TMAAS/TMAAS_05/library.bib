@article{Galindo2013,
abstract = {The study of progress is not recent. Greek thinkers pointed out that progress means a progressive advancement of science and technology and, sometimes, this advance is related to happiness. Some literature has identified economic progress with economic growth. However, there is an important difference between both concepts. According to Holcombe, economic growth refers to the quantity of products, while economic progress refers to the quality of them. The objective of this article is to analyze three points of view, Adam Smith, Keynes and Schumpeter, showing the different implications for society, and possible solutions to the problems rose.},
annote = {1.- El progreso, adem{\'{a}}s de referirse al crecimiento econ{\'{o}}mico, tambi{\'{e}}n se asocia con la calidad de dicho crecimiento.
2.- El progreso tiene connotaciones positivas y negativas.
3.- Holcombe (2007): el progreso implica tambi{\'{e}}n la calidad del crecimiento, y ello se consigue principalmente a trav{\'{e}}s de la innovaci{\'{o}}n, la cual es impulsada por los empresarios. Por tanto, el emprendimiento es el principal factor de progreso econ{\'{o}}mico (tambi{\'{e}}n otros elementos como los aspectos morales y la {\'{e}}tica) junto a Schumpeter.
4.- El ego{\'{i}}smo como elemento inherente a la mano invisible que impulsa a las actividades empresariales mediante nuevas trecnolog{\'{i}}as, innovaciones y productos. 
5.- Progreso econ{\'{o}}mico a trav{\'{e}}s de la acumulaci{\'{o}}n de capital y la tencolog{\'{i}}a, todo ello impulsado, si es necesario, por el gasto p{\'{u}}blico (cuidado con el efecto expulsi{\'{o}}n).
6.- Para Schumpeter, impulsor de la concepci{\'{o}}n cualitativa del proceso econ{\'{o}}mico, en t{\'{e}}rminos de innovaci{\'{o}}n, ello depende del empresario/emprendedor el cual introduce dicha innovaci{\'{o}}n en el proceso productivo.
7.- Para impulsar la actividad emprendedora, Schumpeter considera necesario dos aspectos, el beneficio y el clima social. Este clima social podr{\'{i}}a verse representado por el nivel de renta de un pa{\'{i}}s o por incluso valores morales y {\'{e}}ticos imperantes.
8.- Principio de compensaci{\'{o}}n ante el ego{\'{i}}smo del mercado que hace que unos pocos se vean perjudicados; control del consumismo mediante el fomento de capacidades; la igualdad es clave para evitar tensiones sociales en el clima que fomenta la actividad emprendededora y con ello el progreso econ{\'{o}}mico.
9.- No siempre lo mejor es tomar decisiones cortoplacistas.},
author = {Galindo, Miguel {\'{A}}. and M{\'{e}}ndez, Mar{\'{i}}a T.},
file = {:C$\backslash$:/Users/USUARIO/Documents/UCLM/4{\textordmasculine} Curso ADE/(2) TFG/Bibliograf{\'{i}}a/Bibliografia{\_}MAGT/Galindo, M. {\'{A}}. y M{\'{e}}ndez, M. T. (2013). Progreso econ{\'{o}}mico diferentes planteamientos y aspectos morales y sociales.pdf:pdf},
issn = {15760162},
journal = {Revista de Economia Mundial},
keywords = {A. Smith,Economic Growth,Keynes,Progress,Schumpeter},
number = {35},
pages = {217--227},
title = {{Progreso econ{\'{o}}mico: diferentes planteamientos y aspectos morales y sociales}},
year = {2013}
}
@article{Carranza2020,
abstract = {Purpose: The development of Information Systems (IS) and Information and Communication Technologies (ICT) is offering new opportunities for businesses to implement promotion strategies focused on customer attraction and retention. In this sense, mobile coupon usage has increased as a promotion tool, especially in the fast-food sector. However, the use by consumers of these coupons is not homogeneous and it is conditioned by prior experience. Thus, this study aimed to examine variations between Fast Food Restaurant (FFR) customers based on their prior experiences with the use of mobile coupon (expert vs novice users). Design/methodology/approach: A sample of 400 fast-food customers was collected using a structured questionnaire. In order to compare the proposed relationships between expert and novice users, a multigroup approach was applied through new, recently proposed evaluation procedures designed for PLS–SEM. Findings: The results show that the two groups of consumers (expert vs novice users) have notable differences regarding the relationship between perceived ease of use and perceived usefulness. This relationship was the strongest in both groups. However, there are no differences found in other aspects considered as antecedents to mobile coupons usage, for instance, usage intention and attitude. Practical implications: This work emphasises the importance of considering differences based on experience between mobile coupon users. Ease of use, perceived consumer utility and increased mobile coupons in apps can be the key to driving effective business strategies based on promotional tactics by FFRs. Likewise, this study can help other researchers in their empirical applications of PLS–SEM analysis. Originality/value: This study is the first to provide an in-depth analysis of differences based on users' experience with mobile coupons at FFRs. It is innovative in its introduction of the consumer's coupon proneness variable.},
author = {Carranza, Roc{\'{i}}o and D{\'{i}}az, Estrella and Mart{\'{i}}n, David and Fern{\'{a}}ndez, Pilar},
file = {:C$\backslash$:/Users/USUARIO/Documents/UCLM/4{\textordmasculine} Curso ADE/(2) TFG/Bibliograf{\'{i}}a/Bibliografia{\_}MAGT/Carranza, R., D{\'{i}}az, E., Mart{\'{i}}n, D. y Fern{\'{a}}ndez, P. (2020). PLS-SEM in business promotion strategies. A multigroup analysis of mobile coupon users using MICOM.pdf:pdf},
journal = {Industrial Management {\&} Data Systems},
keywords = {Experience,Fast-food restaurant (FFR),Invariance assessment,Mobile coupon,Multigroup analysis,Partial least squares structural equation modellin,Technology acceptance model (TAM)},
number = {12},
pages = {2349--2374},
title = {{PLS-SEM in business promotion strategies. A multigroup analysis of mobile coupon users using MICOM}},
volume = {120},
year = {2020}
}
@article{Hasan2010,
abstract = {This paper extends the line of research attempting to link innovation to economic growth by addressing some unexplored questions. Using global patent data, this paper empirically investigates the importance of both the quantity and quality of innovation on economic growth, controlling for past measures of inventive inputs. Moreover, our research examines how innovation inputs can be translated into per capita growth under the various economic structures and stages of economic development. Based on a sample of 58 countries for the period 1980-2003, our empirical results indicate that countries hosting firms with higher quality patents also have higher economic growth. Furthermore, we have some evidence that those countries that increase the level of patenting also witness a concomitant increase in economic growth. {\textcopyright} 2010 Elsevier B.V.},
author = {Hasan, Iftekhar and Tucci, Christopher L.},
file = {:C$\backslash$:/Users/USUARIO/Documents/UCLM/4{\textordmasculine} Curso ADE/(2) TFG/Bibliograf{\'{i}}a/Bibliografia{\_}MAGT/innovation.pdf:pdf},
journal = {Research Policy},
keywords = {Economic growth,Innovation,Patents},
pages = {1264--1276},
publisher = {Elsevier B.V.},
title = {{The innovation-economic growth nexus: global evidence}},
volume = {39},
year = {2010}
}
