@article{Hernandez2020,
abstract = {This paper analyses entrepreneurial orientation as a composite formed of innovation, proactiveness and risk-taking. The empirical data for this study were gathered from a survey sent to hotel managers. The fieldwork was carried out between January and June 2018. The process provided 102 valid questionnaires. Two methods were used: structural equation modelling (PLS-SEM) and fuzzy-set qualitative comparative analysis (fsQCA). This study makes six key contributions and findings. First, the use of these two methods provides robust and reliable results. Second, reliability and validity values for innovativeness, proactiveness and risk-taking are satisfactory. Third, the operationalisation of entrepreneurial orientation using the latent variables of innovativeness, proactiveness and risk-taking is satisfactory in terms of reliability and validity. Fourth, these three dimensions have a positive and significant influence on entrepreneurial orientation. Fifth, innovativeness is the most important dimension of entrepreneurial orientation. Sixth, innovativeness is a necessary and sufficient condition for entrepreneurial orientation.},
author = {Hern{\'{a}}ndez, Felipe and Ibarra, Manuel A. and Ribeiro, Domingo and Mogorr{\'{o}}n, Helena},
file = {:C$\backslash$:/Users/USUARIO/Documents/UCLM/4{\textordmasculine} Curso ADE/(2) TFG/Bibliograf{\'{i}}a/Bibliografia{\_}MAGT/Innovativeness as a determinant of entrepreneurial orientation analysis of the hotel sector.pdf:pdf},
