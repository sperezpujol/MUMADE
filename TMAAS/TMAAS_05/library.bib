author = {Barroso, Carmen and Cepeda, Gabriel and Rold{\'{a}}n, Jos{\'{e}} L.},
file = {:C$\backslash$:/Users/USUARIO/Documents/UCLM/4{\textordmasculine} Curso ADE/(2) TFG/Bibliograf{\'{i}}a/Bibliografia{\_}MAGT/Barroso, C., Cepeda, G. y Rold{\'{a}}n, J. L. (2007). Investigar en econom{\'{i}}a de la empresa ¿partial least squares o modelos basados en la covarianza.pdf:pdf},
journal = {XIX Congreso Anual y XV Congreso Hispano Franc{\'{e}}s de AEDEM},
keywords = {metodolog{\'{i}}a,modelos de ecuaciones estructurales,partial least squares,pls},
