@misc{ParsonsNoseTheater,
abstract = {Mr. Dickens: Artista y hombre de negocios As{\'{i}} cuenta la historia que Charles Dickens, el joven autor de Sketches by Boz, fue entrevistado por Chapman y Hall para escribir las portadas de una serie de ilustraciones de un club de hombres caprichoso por Robert Seymour, un destacado artista londinense, que buscaba hacer un asesinato personal con un editor desesperado y un autor ne{\'{o}}fito. Qu{\'{e}} sorprendidos se sintieron cuando el advenedizo Dickens pidi{\'{o}} unos minutos para considerar su propuesta y sali{\'{o}} de la habitaci{\'{o}}n. Regres{\'{o}} para decir que aceptar{\'{i}}a, pero ten{\'{i}}a condiciones. No solo escrib{\'{i}}a leyendas, sino una historia completa sobre el club de hombres. Estar{\'{i}}a escrito en dieciocho cap{\'{i}}tulos, agrupados en tres. El tercer cap{\'{i}}tulo culminar{\'{i}}a con un giro desgarrador de los acontecimientos, asegurando la venta de la pr{\'{o}}xima serie. Se escribir{\'{i}}a en papel barato y sin encuadernar para que sea asequible. Al cabo de un a{\~{n}}o, y la historia, se publicar{\'{i}}a una edici{\'{o}}n en cuero bellamente encuadernada, y pronto seguir{\'{i}}a una edici{\'{o}}n "de lujo". As{\'{i}} surgieron "The Pickwick Papers", as{\'{i}} como las invenciones de Charles Dickens de la "serie" y el "libro de bolsillo". Y se convirti{\'{o}} en el primer escritor en vender el mismo libro al mismo mecenas tres veces. Charles Dickens, que abandon{\'{o}} su propia educaci{\'{o}}n para trabajar en una f{\'{a}}brica de limpiabotas para salvar a su padre de la prisi{\'{o}}n de deudores, se convirti{\'{o}} en un hombre muy rico. {\'{U}}nase a nosotros para celebrar sus palabras.},
author = {{Parson's Nose Theater}},
title = {{Charles Dickens: entrepreneur}},
url = {https://www.parsonsnose.org/charles-dickens-entrepreneur/}
}
