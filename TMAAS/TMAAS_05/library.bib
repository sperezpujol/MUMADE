author = {Ravier, Adri{\'{a}}n},
file = {:C$\backslash$:/Users/USUARIO/Documents/UCLM/4{\textordmasculine} Curso ADE/(2) TFG/Bibliograf{\'{i}}a/Bibliografia{\_}MAGT/Ravier, A. (2016). El pensamiento de Milton Friedman en el marco de la Escuela De Chicago. Estudios Econ{\'{o}}micos, 66, 121-148..pdf:pdf},
journal = {Estudios Econ{\'{o}}micos},
keywords = {Chicago,Friedman},
number = {66},
pages = {121--148},
title = {{El pensamiento de Milton Friedman en el marco de la Escuela De Chicago}},
year = {2016}
}
