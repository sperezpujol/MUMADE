keywords = {SMEs,base-of-the-pyramid,community entrepreneurship,corporate sustainability,critical management studies,embeddedness,inclusive entrepreneurship,shared value,small and medium-sized enterprises,sustainability entrepreneurship,sustainable development,sustainable entrepreneurship},
number = {2},
