author = {Montiel, Oscar J. and Soto, Argentina},
file = {:C$\backslash$:/Users/USUARIO/Documents/UCLM/4{\textordmasculine} Curso ADE/(2) TFG/Bibliograf{\'{i}}a/Bibliografia{\_}MAGT/Montiel, O.J. y Soto, A. (2020). Un marco exploratorio para el emprendimiento desde una perspectiva evolutiva.pdf:pdf},
